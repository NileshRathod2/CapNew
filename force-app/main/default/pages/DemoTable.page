<apex:page controller="demoTableController" >
    <apex:pageBlock title="All Account data">
        <apex:pageBlockTable value="{!accList}" var="acc">
        <apex:column value="{!acc.name}"/>
         <apex:column value="{!acc.rating}"/>
         <apex:column value="{!acc.phone}"/>
        </apex:pageBlockTable>
    </apex:pageBlock>
    <apex:pageBlock title="All Opportunity data">
        <apex:pageBlockTable value="{!oppList}" var="opp">
         <apex:column value="{!opp.name}"/>
          <apex:column value="{!opp.type}"/>
           <apex:column value="{!opp.leadsource}"/>
        </apex:pageBlockTable>
    </apex:pageBlock>
     <apex:pageBlock title="All contact data">
        <apex:pageBlockTable value="{!conList}" var="con">
         <apex:column value="{!con.name}"/>
          <apex:column value="{!con.department}"/>
           <apex:column value="{!con.phone}"/>
           <apex:column value="{!con.leadsource}"/>
        </apex:pageBlockTable>
    </apex:pageBlock>
    
    
</apex:page>